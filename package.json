{
  "name": "zipit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "bunx dotenv -e .env -- prisma generate",
    "migrate:dev": "bunx dotenv -e .env -- prisma migrate dev",
    "prisma:studio": "bunx dotenv -e .env -- prisma studio",
    "migrate": "bunx dotenv -e .env -- prisma migrate deploy",
    "build:production": "bunx dotenv -e .env -- bun run migrate && bun install && bun run build && bun start",
    "prepare": "husky",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "lucide-react": "^0.363.0",
    "next": "14.1.4",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "import-sort-cli": "^6.0.0",
    "postcss": "^8",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
